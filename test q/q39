def missingNumber(nums):
    n = len(nums)
    total_sum = n * (n + 1) // 2
    array_sum = sum(nums)
    return total_sum - array_sum

# Example usage
nums1 = [3, 0, 1]
print(missingNumber(nums1))  # Output: 2

nums2 = [0, 1]
print(missingNumber(nums2))  # Output: 2

nums3 = [9,6,4,2,3,5,7,0,1]
print(missingNumber(nums3))  # Output: 8
